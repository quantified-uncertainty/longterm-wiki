    "discord.js": "^14.16.3",
    "dotenv": "^16.4.7",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
