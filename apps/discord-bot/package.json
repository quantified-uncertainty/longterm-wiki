{
  "name": "discord-bot",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "tsx src/index.ts",
    "test": "vitest run",
    "test:query": "tsx src/test-query.ts",
    "test:suite": "tsx src/test-suite.ts",
    "logs": "tsx src/view-logs.ts",
    "logs:all": "tsx src/view-logs.ts --all",
